end_time <- df$time[which(df$`b_ex_75%` < 0)][length(which(df$`b_ex_75%` < 0))] + 1
move_time <- end_time - start_time
}else{
start_time <- Inf
end_time <- Inf
move_time <- Inf
}
}
mv_time <- data.frame(start_time = start_time, end_time = end_time, move_time = move_time)
df_mv <- rbind(df_mv, mv_time)
mv_time
